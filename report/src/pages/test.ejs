
<div class="container-fluid">
  <div class="row">
    <div class="col">
      <h2 class="mt-2 mb-3"><u><%=groupName + "::" + testName%></u></h2>
    </div>
  </div>
  <div class="row">
    <% ['expected', 'actual', 'diff'].forEach(function(type) { %>
      <div class="col-4">
        <div class="card">
          <a href='<%= dirname + "/../screenshots/" + type + "/" + groupName +  "_" + testName + ".png" %>'>
            <img class="card-img-top" src="<%= dirname + "/../screenshots/" + type + "/" + groupName +  "_" + testName + ".png" %>" alt="<%= type %>">
          </a>
          <div class="card-body">
            <h5 class="card-subtitle"><%= type %></p></h5>
          </div>
        </div>
      </div>
    <% }) %>
  </div>
  <div class="row mt-1">
    <div class="col">
      <div class="border rounded">
        <p>
          <% var date = new Date(test['$']['timeCompleted']) %>
          Time ran: <%=date.getMonth()+1%>/<%=date.getDate()%>/<%=date.getFullYear()%> <%=date.getHours()%>:<%=date.getMinutes()%>:<%=date.getSeconds()%> <br/>
          <br/>
          Number of mismatched pixels: <%= test['$']['numMismatch'] %>
        </p>
      </div>
    </div>
  </div>
</div>
